<form class="runnerCard {{#if system.isDarkMode}}black{{/if}}" autocomplete="off" style="display: {{system.view}};">
  <header>
    <section class="persona {{#if system.angle.hunted}}hunted{{/if}}">
      <div class="angle {{#if system.angle.isOpen}}expan{{/if}}">
        <input class="value" name="system.angle.for" type="text" placeholder="Why you runnig for?" value="{{system.angle.for}}"/>
        {{editor system.angle.DETAILS target="system.angle.DETAILS" button=true owner=owner editable=editable}}
        <div class="bar">
          <div class="DEBT" style="--progress: {{system.angle.DEBT.value}};">ДОЛГ</div>
          <input id="{{actor._id}}_hunted" name="system.angle.hunted" type="checkbox" {{#if system.angle.hunted}}checked{{/if}}/>
          <label for="{{actor._id}}_hunted">ПРЕСЛЕДУЕМЫЙ</label>
          <div class="CRED" style="--progress: {{system.angle.CRED.value}};">КРЕД</div>
        </div>
        <div class="headerButton">
          <input id="{{actor._id}}_angleOpen" class="value" name="system.angle.isOpen" type="checkbox" {{#if system.angle.isOpen}}checked{{/if}}/>
          <label class="angleOpen" for="{{actor._id}}_angleOpen">{{#if system.angle.isOpen}}Collapse{{else}}Expand{{/if}}</label>
          <input id="{{actor._id}}_toggleDarkMode" type="checkbox" name="system.isDarkMode" {{#if system.isDarkMode}}checked{{/if}}>
          <label for="{{actor._id}}_toggleDarkMode">
            {{#if system.isDarkMode}}
            <i class="fa-solid fa-moon"></i>
            {{else}}
            <i class="fa-solid fa-sun"></i>
            {{/if}}
          </label>
        </div>
        
      </div>
      <img src="{{actor.img}}" title="{{actor.name}}" data-edit="img"/>
      <h1><input name="name" type="text" placeholder="Name" value="{{actor.name}}"/></h1>
    </section>
    <section class="roll">
      <div>
        <div class="efect">
          <input name="efect" type="checkbox" id="{{actor._id}}_efect-1" {{#if (isEqual system.roll.efect 1) }}checked{{/if}}/><label for="{{actor._id}}_efect-1">1</label>
          <input name="efect" type="checkbox" id="{{actor._id}}_efect-2" {{#if (isEqual system.roll.efect 2) }}checked{{/if}}/><label for="{{actor._id}}_efect-2">2</label>
          <input name="efect" type="checkbox" id="{{actor._id}}_efect-3" {{#if (isEqual system.roll.efect 3) }}checked{{/if}}/><label for="{{actor._id}}_efect-3">3</label>
          <input name="efect" type="checkbox" id="{{actor._id}}_efect-4" {{#if (isEqual system.roll.efect 4) }}checked{{/if}}/><label for="{{actor._id}}_efect-4">4</label>
        </div>
        <div class="threat">
          <input name="threat" type="checkbox" id="{{actor._id}}_threat-1" {{#if (isEqual system.roll.threat 1) }}checked{{/if}}/><label for="{{actor._id}}_threat-1">1</label>
          <input name="threat" type="checkbox" id="{{actor._id}}_threat-2" {{#if (isEqual system.roll.threat 2) }}checked{{/if}}/><label for="{{actor._id}}_threat-2">2</label>
          <input name="threat" type="checkbox" id="{{actor._id}}_threat-3" {{#if (isEqual system.roll.threat 3) }}checked{{/if}}/><label for="{{actor._id}}_threat-3">3</label>
          <input name="threat" type="checkbox" id="{{actor._id}}_threat-4" {{#if (isEqual system.roll.threat 4) }}checked{{/if}}/><label for="{{actor._id}}_threat-4">4</label>
        </div>
      </div>
      <div class="addDice">
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice-4" {{#if (isEqual system.roll.addDice -4) }}checked{{/if}}/><label for="{{actor._id}}_addDice-4">-4</label>
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice-3" {{#if (isEqual system.roll.addDice -3) }}checked{{/if}}/><label for="{{actor._id}}_addDice-3">-3</label>
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice-2" {{#if (isEqual system.roll.addDice -2) }}checked{{/if}}/><label for="{{actor._id}}_addDice-2">-2</label>
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice-1" {{#if (isEqual system.roll.addDice -1) }}checked{{/if}}/><label for="{{actor._id}}_addDice-1">-1</label>
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice+0" {{#if (isEqual system.roll.addDice 0) }}checked{{/if}}/><label for="{{actor._id}}_addDice+0">+0</label>
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice+1" {{#if (isEqual system.roll.addDice 1) }}checked{{/if}}/><label for="{{actor._id}}_addDice+1">+1</label>
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice+2" {{#if (isEqual system.roll.addDice 2) }}checked{{/if}}/><label for="{{actor._id}}_addDice+2">+2</label>
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice+3" {{#if (isEqual system.roll.addDice 3) }}checked{{/if}}/><label for="{{actor._id}}_addDice+3">+3</label>
        <input name="addDice" type="checkbox" id="{{actor._id}}_addDice+4" {{#if (isEqual system.roll.addDice 4) }}checked{{/if}}/><label for="{{actor._id}}_addDice+4">+4</label>
      </div>
      <div class="buttons">
        <button id="{{actor._id}}_resistRoll" class="resistRoll">СОПРОТИВЛЕНИЕ</button>
        <button id="{{actor._id}}_actionRoll" class="actionRoll">БРОСОК</button>
        <button id="{{actor._id}}_breathRoll" class="breathRoll">ПЕРЕДЫШКА</button>
      </div>
    </div>
  </header>
  <section class="stress">    
    <input name="stress" type="checkbox" id="{{actor._id}}_stres-1" {{#if (isBigger system.stress.value 1) }}checked{{/if}}/><label for="{{actor._id}}_stres-1"></label>
    <input name="stress" type="checkbox" id="{{actor._id}}_stres-2" {{#if (isBigger system.stress.value 2) }}checked{{/if}}/><label for="{{actor._id}}_stres-2"></label>
    <input name="stress" type="checkbox" id="{{actor._id}}_stres-3" {{#if (isBigger system.stress.value 3) }}checked{{/if}}/><label for="{{actor._id}}_stres-3"></label>
    <input name="stress" type="checkbox" id="{{actor._id}}_stres-4" {{#if (isBigger system.stress.value 4) }}checked{{/if}}/><label for="{{actor._id}}_stres-4"></label>
    <input name="stress" type="checkbox" id="{{actor._id}}_stres-5" {{#if (isBigger system.stress.value 5) }}checked{{/if}}/><label for="{{actor._id}}_stres-5"></label>
    <input name="stress" type="checkbox" id="{{actor._id}}_stres-6" {{#if (isBigger system.stress.value 6) }}checked{{/if}}/><label for="{{actor._id}}_stres-6"></label>
    <input name="stress" type="checkbox" id="{{actor._id}}_stres-7" {{#if (isBigger system.stress.value 7) }}checked{{/if}}/><label for="{{actor._id}}_stres-7"></label>
    <p class="{{#if system.stress.isLOAD}}active{{/if}}">ПЕРЕГРУЗКА</p>
  </section>
  <section class="approach">
    <div class="{{#if system.approach.AGGRESSIVE.GLICHED}}GLICHED{{/if}} {{#if (isEqual system.roll.approach "AGGRESSIVE")}}select{{/if}}" data-value="{{system.approach.AGGRESSIVE.dice}}">
      <h3 data-text="HESITATION">АГРЕССИВНО</h3>
      <div class="texts">
        <p>СИЛА ЗАПУГИВАНИЕ РАЗРУШЕНИЕ</p>
        <p> НЕРЕШИТЕЛЬНОСТЬ </p>
      </div>
      <div class="dots">
        <div class="box"></div>
        <div class="box"></div>
      </div>
    </div>
    <div class="{{#if system.approach.SMART.GLICHED}}GLICHED{{/if}} {{#if (isEqual system.roll.approach "SMART")}}select{{/if}}" data-value="{{system.approach.SMART.dice}}">
      <h3 data-text="DISORIENTATION">УМНО</h3>
      <div class="texts">
        <p>КРЕАТИВНОСТЬ ХИТРОСТЬ ПОНИМАНИЕ</p>
        <p> ДЕЗОРИЕНТАЦИЯ </p>
      </div>
      <div class="dots">
        <div class="box"></div>
        <div class="box"></div>
      </div>
    </div>
    <div class="{{#if system.approach.EMPATHIC.GLICHED}}GLICHED{{/if}} {{#if (isEqual system.roll.approach "EMPATHIC")}}select{{/if}}" data-value="{{system.approach.EMPATHIC.dice}}">
      <h3 data-text="APATHY">ЭМПАТИЧНО</h3>
      <div class="texts">
        <p>ХАРИЗМА ЛИДЕРСТВО ДИПЛОМАТИЯ</p>
        <p> АПАТИЯ </p>
      </div>
      <div class="dots">
        <div class="box"></div>
        <div class="box"></div>
      </div>
    </div>
    <div class="{{#if system.approach.STEALTHY.GLICHED}}GLICHED{{/if}} {{#if (isEqual system.roll.approach "STEALTHY")}}select{{/if}}" data-value="{{system.approach.STEALTHY.dice}}">
      <h3 data-text="RECKLESSNESS">СКРЫТНО</h3>
      <div class="texts">
        <p>ОСТРОЖНОСТЬ ХИТРОСТЬ СЕКРЕТНОСТЬ</p>
        <p> БЕЗРАССУДСТВО </p>
      </div>
      <div class="dots">
        <div class="box"></div>
        <div class="box"></div>
      </div>
    </div>
  </section>
  <section class="skills">
    <table>
      <tr data-gliched="{{system.skills.ANALYZE.gliched}}" data-value="{{system.skills.ANALYZE.dice}}" class="{{#if (isEqual system.roll.skill "ANALYZE")}}select{{/if}}">
        <td><h3>АНАЛИЗ</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.ANALYZE.EXPERTISES.PEOPLE}}active{{/if}}">ЛЮДИ</span>,
          <span class="{{#if system.skills.ANALYZE.EXPERTISES.SITUATION}}active{{/if}}">СИТУАЦИЯ</span>,
          <span class="{{#if system.skills.ANALYZE.EXPERTISES.INFORMATION}}active{{/if}}">ИНФОРМАЦИЯ</span>
        </p></td>
      </tr>
      <tr data-gliched="{{system.skills.ATHLETICS.gliched}}" data-value="{{system.skills.ATHLETICS.dice}}"  class="{{#if (isEqual system.roll.skill "ATHLETICS")}}select{{/if}}">
        <td><h3>АТЛЕТИКА</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.ATHLETICS.EXPERTISES.SPRINT}}active{{/if}}">БЕГ</span>,
          <span class="{{#if system.skills.ATHLETICS.EXPERTISES.PARKOUR}}active{{/if}}">ПАРКУР</span>,
          <span class="{{#if system.skills.ATHLETICS.EXPERTISES.SWIM}}active{{/if}}">ПЛАВАНИЕ</span>,
          <span class="{{#if system.skills.ATHLETICS.EXPERTISES.CLIMB}}active{{/if}}">ЛАЗАНИЕ</span>
        </p></td>
      </tr>
      <tr data-gliched="{{system.skills.CLOSE.gliched}}" data-value="{{system.skills.CLOSE.dice}}"  class="{{#if (isEqual system.roll.skill "CLOSE")}}select{{/if}}">
        <td><h3>БЛИЖНИЙ БОЙ</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.CLOSE.EXPERTISES.BRAWLING}}active{{/if}}">БОРЕЦ</span>,
          <span class="{{#if system.skills.CLOSE.EXPERTISES.KNIFE}}active{{/if}}">НОЖИ</span>,
          <span class="{{#if system.skills.CLOSE.EXPERTISES.SWORD}}active{{/if}}">МЕЧ</span>,
          <span class="{{#if system.skills.CLOSE.EXPERTISES.BAT}}active{{/if}}">БИТА</span>,
          <span class="{{#if system.skills.CLOSE.EXPERTISES.WIRE}}active{{/if}}">МОНОСТРУНА</span>
        </p></td>
      </tr>
      <tr data-gliched="{{system.skills.CODING.gliched}}" data-value="{{system.skills.CODING.dice}}"  class="{{#if (isEqual system.roll.skill "CODING")}}select{{/if}}">
        <td><h3>КОДИНГ</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.CODING.EXPERTISES.HACK}}active{{/if}}">ХАКИНГ</span>,
          <span class="{{#if system.skills.CODING.EXPERTISES.COMPILE}}active{{/if}}">КОМПИЛЯЦИЯ</span>,
          <span class="{{#if system.skills.CODING.EXPERTISES.MINE}}active{{/if}}">МАЙНИНГ</span>,
          <span class="{{#if system.skills.CODING.EXPERTISES.SCRAMBLE}}active{{/if}}">ШИФРОВАНИЕ</span>
        </p></td>
      </tr>
      <tr data-gliched="{{system.skills.INFLUENCE.gliched}}" data-value="{{system.skills.INFLUENCE.dice}}"  class="{{#if (isEqual system.roll.skill "INFLUENCE")}}select{{/if}}">
        <td><h3>МАНИПУЛЯЦИЯ</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.INFLUENCE.EXPERTISES.PERFORM}}active{{/if}}">ВЫСТУПЛЕНИЕ</span>,
          <span class="{{#if system.skills.INFLUENCE.EXPERTISES.NEGOTIATE}}active{{/if}}">ПЕРЕГОВОРЫ</span>,
          <span class="{{#if system.skills.INFLUENCE.EXPERTISES.INTIMIDATE}}active{{/if}}">ЗАПУГИВАНИЕ</span>
        </p></td>
      </tr>
      <tr  data-gliched="{{system.skills.PILOTING.gliched}}" data-value="{{system.skills.PILOTING.dice}}"  class="{{#if (isEqual system.roll.skill "PILOTING")}}select{{/if}}">
        <td><h3>УПРАВЛЕНИЕ</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.PILOTING.EXPERTISES.CARS}}active{{/if}}">ТАЧКИ</span>,
          <span class="{{#if system.skills.PILOTING.EXPERTISES.BIKES}}active{{/if}}">БАЙКИ</span>,
          <span class="{{#if system.skills.PILOTING.EXPERTISES.CHOPPERS}}active{{/if}}">ЧОППЕРЫ</span>,
          <span class="{{#if system.skills.PILOTING.EXPERTISES.DRONES}}active{{/if}}">ДРОНЫ</span>
        </p></td>
      </tr>
      <tr data-gliched="{{system.skills.RANGED.gliched}}" data-value="{{system.skills.RANGED.dice}}"  class="{{#if (isEqual system.roll.skill "RANGED")}}select{{/if}}">
        <td><h3>ДАЛЬНИЙ БОЙ</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.RANGED.EXPERTISES.PISTOL}}active{{/if}}">ПИСТОЛЕТ</span>,
          <span class="{{#if system.skills.RANGED.EXPERTISES.RIFLE}}active{{/if}}">ВИНТОВКА</span>,
          <span class="{{#if system.skills.RANGED.EXPERTISES.SMG}}active{{/if}}">П-ПУЛЕМЕТ</span>,
          <span class="{{#if system.skills.RANGED.EXPERTISES.THROWING}}active{{/if}}">МЕТАТЕЛЬНОЕ</span>
        </p></td>
      </tr>
      <tr data-gliched="{{system.skills.RIGGING.gliched}}" data-value="{{system.skills.RIGGING.dice}}"  class="{{#if (isEqual system.roll.skill "RIGGING")}}select{{/if}}">
        <td><h3>ПОЧИНКА</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.RIGGING.EXPERTISES.ELETRONICS}}active{{/if}}">ЭЛЕКТРОНИКА</span>,
          <span class="{{#if system.skills.RIGGING.EXPERTISES.ROBOTICS}}active{{/if}}">РОБОТЫ</span>,
          <span class="{{#if system.skills.RIGGING.EXPERTISES.HEAVY}}active{{/if}}">МЕХАНИЗМЫ</span>
        </p></td>
      </tr>
      <tr data-gliched="{{system.skills.SCIENCES.gliched}}" data-value="{{system.skills.SCIENCES.dice}}"  class="{{#if (isEqual system.roll.skill "SCIENCES")}}select{{/if}}">
        <td><h3>НАУКИ</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.SCIENCES.EXPERTISES.BIOTECH}}active{{/if}}">БИОТЕХ</span>,
          <span class="{{#if system.skills.SCIENCES.EXPERTISES.PHYSICS}}active{{/if}}">ФИЗИКА</span>,
          <span class="{{#if system.skills.SCIENCES.EXPERTISES.CHEMISTRY}}active{{/if}}">ХИМИЯ</span>
        </p></td>
      </tr>
      <tr data-gliched="{{system.skills.STREETWISE.gliched}}" data-value="{{system.skills.STREETWISE.dice}}"  class="{{#if (isEqual system.roll.skill "STREETWISE")}}select{{/if}}">
        <td><h3>ЗНАНИЕ УЛИЦ</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.STREETWISE.EXPERTISES.POLITICS}}active{{/if}}">ПОЛИТИКА</span>,
          <span class="{{#if system.skills.STREETWISE.EXPERTISES.MARKET}}active{{/if}}">РЫНОК</span>,
          <span class="{{#if system.skills.STREETWISE.EXPERTISES.NAVIGATION}}active{{/if}}">НАВИГАЦИЯ</span>
        </p></td>
      </tr>
      {{#if system.wierd}}
      <tr data-gliched="{{system.skills.ATTUNE.gliched}}" data-value="{{system.skills.ATTUNE.dice}}"  class="{{#if (isEqual system.roll.skill "ATTUNE")}}select{{/if}}">
        <td><h3>РЕЗОНАНС</h3></td>
        <td><div class="dots">
          <div class="box"></div>
          <div class="box"></div>
        </div></td>
        <td><p>
          <span class="{{#if system.skills.ATTUNE.EXPERTISES.SPIRITS}}active{{/if}}">ДУХИ</span>,
          <span class="{{#if system.skills.ATTUNE.EXPERTISES.ARTIFACTS}}active{{/if}}">АРТЕФАКТЫ</span>,
          <span class="{{#if system.skills.ATTUNE.EXPERTISES.PLACES}}active{{/if}}">МЕСТА</span>
        </p></td>
      </tr>
      {{/if}}
    </table>
  </section>
  <section class="augmentations">
    <h3>АУГМЕНТАЦИЯ</h3>
    <div class="{{#if system.AUGMENTATIONS.first.GLICHED}}GLICHED{{/if}}">
      <input id="{{actor._id}}_aug-1" type="checkbox" name="system.AUGMENTATIONS.first.isOpen" {{#if system.AUGMENTATIONS.first.isOpen}}checked{{/if}}/>
      <div class="text">
        <label class="name" for="aug-1">
          <input type="text" name="system.AUGMENTATIONS.first.name" value="{{system.AUGMENTATIONS.first.name}}"/>
        </label>
        <p>ГЛЮК</p>
      </div>
      <div class="desc">
        {{editor system.AUGMENTATIONS.first.desc target="system.AUGMENTATIONS.first.desc" button=true owner=owner editable=editable}}
      </div>
    </div>
    <div class="{{#if system.AUGMENTATIONS.second.GLICHED}}GLICHED{{/if}}">
      <input id="{{actor._id}}_aug-2" type="checkbox" name="system.AUGMENTATIONS.second.isOpen" {{#if system.AUGMENTATIONS.second.isOpen}}checked{{/if}}/>
      <div class="text">
        <label class="name" for="aug-2">
          <input type="text" name="system.AUGMENTATIONS.second.name" value="{{system.AUGMENTATIONS.second.name}}"/>
        </label>
        <p>ГЛЮК</p>
      </div>
      <div class="desc">
        {{editor system.AUGMENTATIONS.second.desc target="system.AUGMENTATIONS.second.desc" button=true owner=owner editable=editable}}
      </div>
    </div>
    <div class="{{#if system.AUGMENTATIONS.third.GLICHED}}GLICHED{{/if}}">
      <input id="{{actor._id}}_aug-3" type="checkbox" name="system.AUGMENTATIONS.third.isOpen" {{#if system.AUGMENTATIONS.third.isOpen}}checked{{/if}}/>
      <div class="text">
        <label class="name" for="aug-3">
          <input type="text" name="system.AUGMENTATIONS.third.name" value="{{system.AUGMENTATIONS.third.name}}"/>
        </label>
        <p>ГЛЮК</p>
      </div>
      <div class="desc">
        {{editor system.AUGMENTATIONS.third.desc target="system.AUGMENTATIONS.third.desc" button=true owner=owner editable=editable}}
      </div>
    </div>
  </section>
  <section class="gear">
    <h3>
      СНАРЯГА: {{system.GEAR.LOAD.value}} /
      {{#if (isEqual system.GEAR.LOAD.selected "light")}}
        {{system.GEAR.LOAD.light.value}}
      {{/if}}
      {{#if (isEqual system.GEAR.LOAD.selected "medium")}}
        {{system.GEAR.LOAD.medium.value}}
      {{/if}}
      {{#if (isEqual system.GEAR.LOAD.selected "heavy")}}
        {{system.GEAR.LOAD.heavy.value}}
      {{/if}}
    </h3>
    <div>
      <button class="load">
      {{#if (isEqual system.GEAR.LOAD.selected "light")}}
        ЛЁГКАЯ: {{system.GEAR.LOAD.light.desc}}
      {{/if}}
      {{#if (isEqual system.GEAR.LOAD.selected "medium")}}
        СРЕДНЯЯ: {{system.GEAR.LOAD.medium.desc}}
      {{/if}}
      {{#if (isEqual system.GEAR.LOAD.selected "heavy")}}
        ТЯЖЕЛАЯ: {{system.GEAR.LOAD.heavy.desc}}
      {{/if}}
    </button>
    </div>
    <div>
        <input id="{{actor._id}}_eq1" type="checkbox" name="eq1" {{#if system.GEAR.Equpment.eq1.isUse}}checked{{/if}}/>
        <label for="{{actor._id}}_eq1" data-id="eq1">
          <div><div class="box"></div></div>
          КЛИНОК ИЛИ ДВА
        </label>
    </div>
    <div>
        <input id="{{actor._id}}_eq2" type="checkbox" name="eq2" {{#if system.GEAR.Equpment.eq2.isUse}}checked{{/if}}/>
        <label for="{{actor._id}}_eq2" data-id="eq2">
          <div><div class="box"></div></div>
          ЛЁГКОЕ ОРУЖИЕ
        </label>
    </div>
